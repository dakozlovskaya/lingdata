    <TIER ANNOTATOR="@dakozlovskaya" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="words" PARENT_REF="text@vis1933f"
        PARTICIPANT="@vis1933f" TIER_ID="transl@vis1933f">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>They used to come to us from Khamovka... A fight...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>They have always started fights.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>They have fighted. Sometimes even people were killed.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a35"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>Every Nicola's day a person will surely be killed. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a36"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>They are going to kill someone.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>Exciting. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>We had fun... they all wrestled</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a42"
                TIME_SLOT_REF1="ts24" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> They were such... They got drunk.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a43"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>So many things they were doing. These ones... from Khamovchiki.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a44"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>I'm sitskarka</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a46"
                TIME_SLOT_REF1="ts32" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>I am a sitskarian woman.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a47"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>Sitskari are living here.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a48"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>All my fellows were working.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a49"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>They all were professionals.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a50"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>They were building houses.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a51"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>Carpenters.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a52"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>All of them were carpenters.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a53"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE> And women were weaving rugs.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a54"
                TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>We have a river Sit nearby our village.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a55"
                TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE>So sometimes we go fishing.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a56"
                TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE>We catch  fish. Earlier we had more fish.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="@dakozlovskaya" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="words" PARENT_REF="text@axn1980m"
        PARTICIPANT="@axn1980m" TIER_ID="transl@axn1980m">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>All districts knew about difficult character of the Sitskari, and the Russian neighbors, as they were called here by the khamums or khamovshchiki, tried to pass by these places, especially on holidays: Nikolin or Ilyin's day.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>The oldest resident of the village, Valentina Ivanovna Smirnova, remembers well these dashing times.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a37"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>Well, you had fun, I see...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a40"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> And why were they fighting? </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a41"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts25">
                <ANNOTATION_VALUE>Warlike nature or..</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
